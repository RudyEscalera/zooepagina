<h1>Listing hogar_temporals</h1>

<table>
  <thead>
    <tr>
      <th>Ci</th>
      <th>Nombres</th>
      <th>Apellidos</th>
      <th>Telefono fijo</th>
      <th>Direccion</th>
      <th>Numero</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @hogar_temporals.each do |hogar_temporal| %>
      <tr>
        <td><%= hogar_temporal.ci %></td>
        <td><%= hogar_temporal.nombres %></td>
        <td><%= hogar_temporal.apellidos %></td>
        <td><%= hogar_temporal.telefono_fijo %></td>
        <td><%= hogar_temporal.direccion %></td>
        <td><%= hogar_temporal.numero %></td>
        <td><%= link_to 'Show', hogar_temporal %></td>
        <td><%= link_to 'Edit', edit_hogar_temporal_path(hogar_temporal) %></td>
        <td><%= link_to 'Destroy', hogar_temporal, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Hogar temporal', new_hogar_temporal_path %>
