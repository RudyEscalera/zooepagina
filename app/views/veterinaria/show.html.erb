<div align="left"> 
<div class = "formulario">
<div style="clear:both"></div>
<h1>Veterinaria : <%= @veterinarium.nombre %></h1>
<p id="notice"><%= notice %></p>
<div style="clear:both"></div>
<div>
  <strong>Nombre:</strong></div>
  <div><%= @veterinarium.nombre %></div>
<div style="clear:both"></div>

<div>
  <strong>Direccion:</strong></div>
  <div><%= @veterinarium.direccion %></div>
<div style="clear:both"></div>

<div>
  <strong>Telefono:</strong></div>
  <div><%= @veterinarium.telefono %></div>
<div style="clear:both"></div>

<div>
  <strong>Movil:</strong></div>
  <div><%= @veterinarium.movil %></div>
  <div style="clear:both"></div>
<div>
  <strong>Encargado:</strong></div>
  <div><%= @veterinarium.encargado %></div>
  <div style="clear:both"></div>

  <% if user_signed_in? %>
          <% if current_user.estado?  %>
  <div><strong>Activo:</strong></div>
  <div><%= @veterinarium.activo %></div>
  <div style="clear:both"></div>
  <% end -%>
  <% end -%>
<div>
  <strong>Descripcion:</strong></div>
  <div><%= @veterinarium.descripcion %></div>
  <div style="clear:both"></div>
  <% if user_signed_in? %>
          <% if current_user.estado?  %>
  <div class="actions">
<%= link_to 'Editar', edit_veterinarium_path(@veterinarium), :class=>"btn btn-success"  %> 
<% end -%>
<% end -%>
</div>
</div>
</div>

